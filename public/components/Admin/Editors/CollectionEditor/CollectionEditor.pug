extends ../../../../common/Admin/ItemEditor/ItemEditor

block content
	div(class="form-edit__container")
		label(class="form-edit__label")="Name"
		input(class="form-edit__input" type="text" name="name" id='collection-name-input' value=item.name)
	div(class="form-edit__container")
		label(class="form-edit__label")="Upload image of collection:"
		input(class="form-edit__input" type="file" name="file" value=item.file)